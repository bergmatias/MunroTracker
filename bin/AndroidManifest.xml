<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.munrotracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.munrotracker.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.munrotracker.Map"
            android:label="@string/title_activity_map"
            android:parentActivityName="com.example.munrotracker.Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.munrotracker.Home" />
        </activity>
        <activity
            android:name="com.example.munrotracker.List"
            android:label="@string/title_activity_list"
            android:parentActivityName="com.example.munrotracker.Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.munrotracker.Home" />
        </activity>
        <activity
            android:name="com.example.munrotracker.Info"
            android:label="@string/title_activity_info"
            android:parentActivityName="com.example.munrotracker.Home" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.munrotracker.Home" />
        </activity>
    
    	<meta-data
		    android:name="com.google.android.maps.v2.API_KEY"
		    android:value="AIzaSyAuj53u7JO7cIZIKfHdmLcK1ZLkpfAZrzk"/>
    </application>

</manifest>
